{
  "name": "cambio",
  "version": "0.0.0-development",
  "description": "Consulta la Tasa Representativa del Mercado en el servicio web de la Superintendencia Financiera de Colombia",
  "main": "src/index.js",
  "engines": {
    "node": ">=8.10.0"
  },
  "scripts": {
    "lint": "eslint src/**/*.js",
    "test": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged",
        "npm t"
      ],
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/archemiro/cambio/issues"
  },
  "homepage": "https://github.com/archemiro/cambio#readme",
  "author": "Mauricio Robayo <archemiro@gmail.com> (https://www.archemiro.com)",
  "repository": {
    "type": "git",
    "url": "git@github.com:archemiro/cambio.git"
  },
  "keywords": [
    "Colombia",
    "Tasa Representativa del Mercado",
    "Tasa de Cambio Representativa del Mercado",
    "tcrm",
    "trm",
    "Superintendencia Financiera de Colombia"
  ],
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "codecov": "^3.1.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^1.1.4",
    "jest": "^23.6.0",
    "lint-staged": "^8.0.4",
    "prettier": "^1.15.1",
    "semantic-release": "^15.12.0"
  },
  "dependencies": {},
  "files": [
    "/src/index.js",
    "/src/request.js"
  ],
  "size-limit": [
    {
      "path": "src/index.js",
      "limit": "9 KB"
    }
  ]
}
